# App
APP_URL=http://localhost:3000
APP_ENCRYPTION_KEY=change_this_to_32_byte_key_min_length_32_chars

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Database (Prisma)
# Connect to Supabase Postgres (Transaction pooler usually port 6543)
DATABASE_URL="postgres://postgres.xxxx:password@aws-0-region.pooler.supabase.com:6543/postgres?pgbouncer=true"
# Direct connection for migrations (Session mode port 5432)
DIRECT_URL="postgres://postgres.xxxx:password@aws-0-region.pooler.supabase.com:5432/postgres"
PRISMA_CLIENT_ENGINE_TYPE=binary

# Redis (BullMQ)
REDIS_URL=redis://localhost:6379

# Instagram
IG_APP_ID=
IG_APP_SECRET=
IG_REDIRECT_URI=http://localhost:3000/api/instagram/callback
IG_VERIFY_TOKEN=creatye_verify_token
